            <argument type="service" id="storefront.media.hydrator"/>
        </service>

        <service id="storefront.configurator.product_configuration_gateway" class="Shopware\Bundle\StoreFrontBundle\Configurator\ProductConfigurationGateway">
            <argument type="service" id="dbal_connection"/>
            <argument type="service" id="storefront.common.field_helper"/>
            <argument type="service" id="storefront.configurator.hydrator"/>
        </service>

        <service id="storefront.manufacturer.gateway" class="Shopware\Bundle\StoreFrontBundle\Manufacturer\ManufacturerGateway">
