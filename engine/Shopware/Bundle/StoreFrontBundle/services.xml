            <argument type="service" id="shopware_search.store_front_criteria_factory"/>
            <argument type="service" id="config" />
        </service>

        <service id="storefront.price.graduated_prices_service" class="Shopware\Bundle\StoreFrontBundle\Price\GraduatedPricesService">
            <argument type="service" id="storefront.price.graduated_prices_gateway"/>
        </service>

        <service id="storefront.manufacturer.service" class="Shopware\Bundle\StoreFrontBundle\Manufacturer\ManufacturerService">
