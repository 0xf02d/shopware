<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="storefront.context.service" class="Shopware\Bundle\StoreFrontBundle\Context\ContextService">
            <argument id="service_container" type="service"/>
            <argument id="storefront.context.factory" type="service"/>
            <argument id="storefront.common.cache" type="service"/>
        </service>

        <service id="storefront.context.factory" class="Shopware\Bundle\StoreFrontBundle\Context\ContextFactory">
            <argument id="storefront.shop.gateway" type="service"/>
            <argument id="storefront.currency.gateway" type="service"/>
            <argument id="storefront.customer.service" type="service"/>
            <argument id="storefront.customer_group.gateway" type="service"/>
            <argument id="storefront.country.gateway" type="service"/>
            <argument id="storefront.tax.gateway" type="service"/>
            <argument id="storefront.price_group.gateway" type="service"/>
            <argument id="storefront.address.gateway" type="service"/>
            <argument id="storefront.payment_method.gateway" type="service"/>
            <argument id="storefront.shipping_method.gateway" type="service"/>
            <argument id="dbal_connection" type="service"/>
        </service>

        <service id="storefront.context.switcher" class="Shopware\Bundle\StoreFrontBundle\Context\ContextSwitcher" >
            <argument id="session" type="service"/>
            <argument id="storefront.context.service" type="service"/>
        </service>

        <service id="storefront.address.hydrator" class="Shopware\Bundle\StoreFrontBundle\Address\AddressHydrator">
            <argument type="service" id="storefront.common.attribute_hydrator" />
            <argument type="service" id="storefront.country.hydrator" />
        </service>

        <service id="storefront.customer.hydrator" class="Shopware\Bundle\StoreFrontBundle\Customer\CustomerHydrator">
            <argument type="service" id="storefront.common.attribute_hydrator" />
            <argument type="service" id="storefront.customer_group.hydrator" />
        </service>

        <service id="storefront.customer.gateway" class="Shopware\Bundle\StoreFrontBundle\Customer\CustomerGateway">
            <argument type="service" id="storefront.common.field_helper" />
            <argument type="service" id="storefront.customer.hydrator" />
            <argument type="service" id="dbal_connection" />
        </service>

        <service id="storefront.address.gateway" class="Shopware\Bundle\StoreFrontBundle\Address\AddressGateway">
            <argument type="service" id="storefront.common.field_helper" />
            <argument type="service" id="storefront.address.hydrator" />
            <argument type="service" id="dbal_connection" />
        </service>

        <service id="storefront.customer.service" class="Shopware\Bundle\StoreFrontBundle\Customer\CustomerService">
            <argument type="service" id="storefront.customer.gateway" />
            <argument type="service" id="storefront.address.gateway" />
            <argument type="service" id="storefront.shop.gateway" />
            <argument type="service" id="storefront.payment_method.gateway" />
        </service>

        <service id="storefront.payment_method.gateway" class="Shopware\Bundle\StoreFrontBundle\PaymentMethod\PaymentMethodGateway">
            <argument type="service" id="storefront.common.field_helper" />
            <argument type="service" id="storefront.payment_method.hydrator" />
            <argument type="service" id="dbal_connection" />
        </service>

        <service id="storefront.payment_method.service" class="Shopware\Bundle\StoreFrontBundle\PaymentMethod\PaymentMethodService">
            <argument id="storefront.payment_method.gateway" type="service"/>
            <argument id="cart.validator.filter" type="service"/>
        </service>

        <service id="storefront.payment_method.hydrator" class="Shopware\Bundle\StoreFrontBundle\PaymentMethod\PaymentMethodHydrator">
            <argument type="service" id="storefront.common.attribute_hydrator" />
            <argument type="service" id="storefront.serializer.json" />
        </service>

        <service id="storefront.shipping_method.service" class="Shopware\Bundle\StoreFrontBundle\ShippingMethod\ShippingMethodService">
            <argument id="storefront.shipping_method.gateway" type="service"/>
        </service>

        <service id="storefront.shipping_method.gateway" class="Shopware\Bundle\StoreFrontBundle\ShippingMethod\ShippingMethodGateway">
            <argument type="service" id="storefront.common.field_helper" />
            <argument type="service" id="storefront.shipping_method.hydrator" />
            <argument type="service" id="dbal_connection" />
        </service>

        <service id="storefront.shipping_method.hydrator" class="Shopware\Bundle\StoreFrontBundle\ShippingMethod\ShippingMethodHydrator">
            <argument type="service" id="storefront.common.attribute_hydrator" />
        </service>

        <service id="storefront.listing.facet_service" class="Shopware\Bundle\StoreFrontBundle\Listing\ListingFacetService">
            <argument type="service" id="storefront.listing.facet_gateway" />
        </service>

        <service id="storefront.listing.sorting_service" class="Shopware\Bundle\StoreFrontBundle\Listing\ListingSortingService">
            <argument type="service" id="storefront.listing.sorting_gateway" />
        </service>

        <service id="storefront.country.service" class="Shopware\Bundle\StoreFrontBundle\Country\CountryService">
            <argument type="service" id="storefront.country.gateway" />
            <argument type="service" id="dbal_connection" />
        </service>

        <service id="storefront.category.depth_service" class="Shopware\Bundle\StoreFrontBundle\Category\CategoryDepthService">
            <argument type="service" id="dbal_connection" />
        </service>

        <service id="storefront.blog.service" class="Shopware\Bundle\StoreFrontBundle\Blog\BlogService">
            <argument type="service" id="storefront.blog.gateway" />
            <argument type="service" id="storefront.media.service" />
        </service>

        <service id="storefront.product.number_service" class="Shopware\Bundle\StoreFrontBundle\Product\ProductNumberService">
            <argument type="service" id="dbal_connection" />
            <argument type="service" id="config" />
        </service>

        <service id="storefront.product.list_product_service" class="Shopware\Bundle\StoreFrontBundle\Product\ListProductService">
            <argument type="service" id="storefront.product.gateway"/>
            <argument type="service" id="storefront.price.graduated_prices_service"/>
            <argument type="service" id="storefront.price.cheapest_price_service"/>
            <argument type="service" id="storefront.price.calculation_service"/>
            <argument type="service" id="storefront.media.service"/>
            <argument type="service" id="storefront.vote.service"/>
            <argument type="service" id="storefront.category.service" />
            <argument type="service" id="config" />
        </service>

        <service id="storefront.product.base_product_factory" class="Shopware\Bundle\StoreFrontBundle\Product\BaseProductFactoryService">
            <argument type="service" id="dbal_connection" />
        </service>

        <service id="storefront.product.service" class="Shopware\Bundle\StoreFrontBundle\Product\ProductService">
            <argument type="service" id="storefront.product.list_product_service"/>
            <argument type="service" id="storefront.vote.service"/>
            <argument type="service" id="storefront.media.service"/>
            <argument type="service" id="storefront.related_product.service"/>
            <argument type="service" id="storefront.related_product.stream_service"/>
            <argument type="service" id="storefront.similar_product.service"/>
            <argument type="service" id="storefront.product_download.service"/>
            <argument type="service" id="storefront.product_link.service"/>
            <argument type="service" id="storefront.property.service"/>
            <argument type="service" id="storefront.configurator.service"/>
            <argument type="service" id="events"/>
        </service>

        <service id="storefront.product_download.service" class="Shopware\Bundle\StoreFrontBundle\ProductDownload\ProductDownloadService">
            <argument type="service" id="storefront.product_download.gateway"/>
        </service>

        <service id="storefront.product_link.service" class="Shopware\Bundle\StoreFrontBundle\ProductLink\ProductLinkService">
            <argument type="service" id="storefront.product_link.gateway"/>
        </service>

        <service id="storefront.related_product.service" class="Shopware\Bundle\StoreFrontBundle\RelatedProduct\RelatedProductService">
            <argument type="service" id="storefront.related_product.gateway"/>
            <argument type="service" id="storefront.product.list_product_service"/>
        </service>

        <service id="storefront.related_product.stream_service" class="Shopware\Bundle\StoreFrontBundle\RelatedProduct\RelatedProductStreamService">
            <argument type="service" id="storefront.related_product.stream_gateway"/>
        </service>

        <service id="storefront.similar_product.service" class="Shopware\Bundle\StoreFrontBundle\SimilarProduct\SimilarProductService">
            <argument type="service" id="storefront.similar_product.gateway"/>
            <argument type="service" id="storefront.product.list_product_service"/>
