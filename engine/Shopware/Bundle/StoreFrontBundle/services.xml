        <service id="storefront.price.cheapest_price_service" class="Shopware\Bundle\StoreFrontBundle\Price\CheapestPriceService">
            <argument type="service" id="storefront.price.cheapest_price_gateway"/>
            <argument type="service" id="config" />
        </service>

        <service id="storefront.media.service" class="Shopware\Bundle\StoreFrontBundle\Media\MediaService">
            <argument type="service" id="storefront.media.gateway"/>
            <argument type="service" id="storefront.media.product_media_gateway"/>
            <argument type="service" id="storefront.media.variant_media_gateway"/>
            <argument type="service" id="config"/>
        </service>

        <service id="storefront.property.service" class="Shopware\Bundle\StoreFrontBundle\Property\PropertyService">
            <argument type="service" id="storefront.property.product_property_gateway"/>
        </service>

        <service id="storefront.configurator.service" class="Shopware\Bundle\StoreFrontBundle\Configurator\ConfiguratorService">
            <argument type="service" id="storefront.configurator.product_configuration_gateway"/>
            <argument type="service" id="storefront.configurator.gateway"/>
        </service>

        <service id="storefront.category.service" class="Shopware\Bundle\StoreFrontBundle\Category\CategoryService">
            <argument type="service" id="storefront.category.gateway"/>
        </service>

        <service id="storefront.vote.service" class="Shopware\Bundle\StoreFrontBundle\Vote\VoteService">
            <argument type="service" id="storefront.vote.gateway"/>
            <argument type="service" id="storefront.vote.average_gateway"/>
        </service>

        <service id="storefront.additional_text.service" class="Shopware\Bundle\StoreFrontBundle\AdditionalText\AdditionalTextService">
            <argument type="service" id="storefront.configurator.service"/>
        </service>


        <!-- DBAL gateway classes, which used as shopware default -->
        <service id="storefront.product.gateway" class="Shopware\Bundle\StoreFrontBundle\Product\ProductGateway">
            <argument type="service" id="dbal_connection"/>
            <argument type="service" id="storefront.common.field_helper"/>
            <argument type="service" id="storefront.product.hydrator"/>
            <argument type="service" id="config" />
        </service>

        <service id="storefront.media.gateway" class="Shopware\Bundle\StoreFrontBundle\Media\MediaGateway">
            <argument type="service" id="dbal_connection"/>
            <argument type="service" id="storefront.common.field_helper"/>
            <argument type="service" id="storefront.media.hydrator"/>
        </service>

        <service id="storefront.media.product_media_gateway" class="Shopware\Bundle\StoreFrontBundle\Media\ProductMediaGateway">
            <argument type="service" id="dbal_connection"/>
            <argument type="service" id="storefront.common.field_helper"/>
            <argument type="service" id="storefront.media.hydrator"/>
        </service>

        <service id="storefront.media.variant_media_gateway" class="Shopware\Bundle\StoreFrontBundle\Media\VariantMediaGateway">
