            <argument type="service" id="dbal_connection"/>
            <argument type="service" id="storefront.common.field_helper"/>
            <argument type="service" id="storefront.product_download.hydrator"/>
        </service>

        <service id="storefront.product_link.gateway" class="Shopware\Bundle\StoreFrontBundle\ProductLink\ProductLinkGateway">
            <argument type="service" id="dbal_connection"/>
            <argument type="service" id="storefront.common.field_helper"/>
            <argument type="service" id="storefront.product_link.hydrator"/>
