            <argument type="service" id="storefront.common.field_helper"/>
            <argument type="service" id="storefront.manufacturer.hydrator"/>
        </service>

        <service id="storefront.shop_page.gateway" class="Shopware\Bundle\StoreFrontBundle\ShopPage\ShopPageGateway">
            <argument type="service" id="dbal_connection"/>
            <argument type="service" id="storefront.common.field_helper"/>
            <argument type="service" id="storefront.shop_page.hydrator"/>
        </service>

        <service id="storefront.tax.gateway" class="Shopware\Bundle\StoreFrontBundle\Tax\TaxGateway">
            <argument type="service" id="dbal_connection"/>
            <argument type="service" id="storefront.common.field_helper"/>
            <argument type="service" id="storefront.tax.hydrator"/>
        </service>

        <service id="storefront.configurator.gateway" class="Shopware\Bundle\StoreFrontBundle\Configurator\ConfiguratorGateway">
            <argument type="service" id="dbal_connection"/>
            <argument type="service" id="storefront.common.field_helper"/>
            <argument type="service" id="storefront.configurator.hydrator"/>
            <argument type="service" id="storefront.media.gateway" />
        </service>

        <service id="storefront.price.graduated_prices_gateway" class="Shopware\Bundle\StoreFrontBundle\Price\GraduatedPricesGateway">
            <argument type="service" id="dbal_connection"/>
            <argument type="service" id="storefront.common.field_helper"/>
            <argument type="service" id="storefront.price.hydrator"/>
        </service>

        <service id="storefront.price.cheapest_price_gateway" class="Shopware\Bundle\StoreFrontBundle\Price\CheapestPriceGateway">
            <argument type="service" id="dbal_connection"/>
            <argument type="service" id="storefront.common.field_helper"/>
            <argument type="service" id="storefront.price.hydrator"/>
            <argument type="service" id="config" />
        </service>

        <service id="storefront.price_group.gateway" class="Shopware\Bundle\StoreFrontBundle\PriceGroup\PriceGroupGateway">
            <argument type="service" id="dbal_connection"/>
            <argument type="service" id="storefront.common.field_helper"/>
            <argument type="service" id="storefront.price.hydrator"/>
        </service>

        <service id="storefront.property.gateway" class="Shopware\Bundle\StoreFrontBundle\Property\PropertyGateway">
            <argument type="service" id="dbal_connection"/>
            <argument type="service" id="storefront.common.field_helper"/>
            <argument type="service" id="storefront.property.hydrator"/>
            <argument type="service" id="config" />
        </service>

        <service id="storefront.property.product_property_gateway" class="Shopware\Bundle\StoreFrontBundle\Property\ProductPropertyGateway">
            <argument type="service" id="dbal_connection"/>
            <argument type="service" id="storefront.common.field_helper"/>
            <argument type="service" id="storefront.property.hydrator"/>
        </service>

        <service id="storefront.related_product.gateway" class="Shopware\Bundle\StoreFrontBundle\RelatedProduct\RelatedProductGateway">
            <argument type="service" id="dbal_connection"/>
        </service>

        <service id="storefront.related_product.stream_gateway" class="Shopware\Bundle\StoreFrontBundle\RelatedProduct\RelatedProductStreamGateway">
            <argument type="service" id="dbal_connection"/>
