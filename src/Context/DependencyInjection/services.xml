<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service class="Shopware\Context\Service\ContextFactory" id="shopware.context.service.context_factory">
            <argument type="service" id="Shopware\Shop\Repository\ShopRepository"/>
            <argument type="service" id="Shopware\Currency\Repository\CurrencyRepository"/>
            <argument type="service" id="Shopware\Customer\Repository\CustomerRepository"/>
            <argument type="service" id="Shopware\Customer\Repository\CustomerGroupRepository"/>
            <argument type="service" id="Shopware\Country\Repository\CountryRepository"/>
            <argument type="service" id="Shopware\Tax\Repository\TaxRepository"/>
            <argument type="service" id="Shopware\Customer\Repository\CustomerAddressRepository"/>
            <argument type="service" id="Shopware\Payment\Repository\PaymentMethodRepository"/>
            <argument type="service" id="Shopware\Shipping\Repository\ShippingMethodRepository"/>
            <argument type="service" id="dbal_connection"/>
            <argument type="service" id="Shopware\Country\Repository\CountryStateRepository"/>
        </service>

    </services>
</container>
