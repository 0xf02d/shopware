<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
         <service class="Shopware\Shipping\Definition\ShippingMethodDefinition" id="shopware.shipping.shipping_method_definition" >
            <tag name="shopware.entity.definition" entity="shipping_method" />
        </service>
        <service class="Shopware\Shipping\Repository\ShippingMethodRepository" id="Shopware\Shipping\Repository\ShippingMethodRepository" public="true">
          <argument id="shopware.api.entity_reader" type="service"/>
          <argument id="shopware.api.entity_writer" type="service"/>
          <argument id="shopware.api.entity_searcher" type="service"/>
          <argument id="shopware.api.entity_aggregator" type="service"/>
          <argument id="event_dispatcher" type="service"/>
        </service>
         <service class="Shopware\Shipping\Definition\ShippingMethodPriceDefinition" id="shopware.shipping.shipping_method_price_definition" >
            <tag name="shopware.entity.definition" entity="shipping_method_price" />
        </service>
        <service class="Shopware\Shipping\Repository\ShippingMethodPriceRepository" id="Shopware\Shipping\Repository\ShippingMethodPriceRepository" public="true">
          <argument id="shopware.api.entity_reader" type="service"/>
          <argument id="shopware.api.entity_writer" type="service"/>
          <argument id="shopware.api.entity_searcher" type="service"/>
          <argument id="shopware.api.entity_aggregator" type="service"/>
          <argument id="event_dispatcher" type="service"/>
        </service>
         <service class="Shopware\Shipping\Definition\ShippingMethodTranslationDefinition" id="shopware.shipping.shipping_method_translation_definition" >
            <tag name="shopware.entity.definition" entity="shipping_method_translation" />
        </service>
        <service class="Shopware\Shipping\Repository\ShippingMethodTranslationRepository" id="Shopware\Shipping\Repository\ShippingMethodTranslationRepository" public="true">
          <argument id="shopware.api.entity_reader" type="service"/>
          <argument id="shopware.api.entity_writer" type="service"/>
          <argument id="shopware.api.entity_searcher" type="service"/>
          <argument id="shopware.api.entity_aggregator" type="service"/>
          <argument id="event_dispatcher" type="service"/>
        </service>
    </services>
</container>                
                