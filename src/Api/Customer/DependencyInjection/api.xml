        <service class="Shopware\Api\Customer\Repository\CustomerGroupRepository" id="Shopware\Api\Customer\Repository\CustomerGroupRepository" public="true">
          <argument id="shopware.api.entity_reader" type="service"/>
          <argument id="shopware.api.entity_writer" type="service"/>
          <argument id="shopware.api.entity_searcher" type="service"/>
          <argument id="shopware.api.entity_aggregator" type="service"/>
          <argument id="event_dispatcher" type="service"/>
        </service>
         <service class="Shopware\Api\Customer\Definition\CustomerGroupDiscountDefinition" id="shopware.customer.customer_group_discount_definition" >
            <tag name="shopware.entity.definition" entity="customer_group_discount" />
        </service>
        <service class="Shopware\Api\Customer\Repository\CustomerGroupDiscountRepository" id="Shopware\Api\Customer\Repository\CustomerGroupDiscountRepository" public="true">
          <argument id="shopware.api.entity_reader" type="service"/>
          <argument id="shopware.api.entity_writer" type="service"/>
          <argument id="shopware.api.entity_searcher" type="service"/>
          <argument id="shopware.api.entity_aggregator" type="service"/>
          <argument id="event_dispatcher" type="service"/>
        </service>
         <service class="Shopware\Api\Customer\Definition\CustomerGroupTranslationDefinition" id="shopware.customer.customer_group_translation_definition" >
            <tag name="shopware.entity.definition" entity="customer_group_translation" />
        </service>
        <service class="Shopware\Api\Customer\Repository\CustomerGroupTranslationRepository" id="Shopware\Api\Customer\Repository\CustomerGroupTranslationRepository" public="true">
          <argument id="shopware.api.entity_reader" type="service"/>
          <argument id="shopware.api.entity_writer" type="service"/>
          <argument id="shopware.api.entity_searcher" type="service"/>
          <argument id="shopware.api.entity_aggregator" type="service"/>
          <argument id="event_dispatcher" type="service"/>
        </service>
    </services>
</container>                
                