          <argument id="shopware.api.entity_searcher" type="service"/>
          <argument id="shopware.api.entity_aggregator" type="service"/>
          <argument id="event_dispatcher" type="service"/>
        </service>
         <service class="Shopware\Api\Customer\Definition\CustomerGroupDefinition" id="shopware.customer.customer_group_definition" >
            <tag name="shopware.entity.definition" entity="customer_group" />
