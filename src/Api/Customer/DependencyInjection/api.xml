        </service>
        <service class="Shopware\Api\Customer\Repository\CustomerRepository" id="Shopware\Api\Customer\Repository\CustomerRepository" public="true">
          <argument id="shopware.api.entity_reader" type="service"/>
          <argument id="shopware.api.entity_writer" type="service"/>
          <argument id="shopware.api.entity_searcher" type="service"/>
          <argument id="shopware.api.entity_aggregator" type="service"/>
          <argument id="event_dispatcher" type="service"/>
        </service>
         <service class="Shopware\Api\Customer\Definition\CustomerAddressDefinition" id="shopware.customer.customer_address_definition" >
            <tag name="shopware.entity.definition" entity="customer_address" />
