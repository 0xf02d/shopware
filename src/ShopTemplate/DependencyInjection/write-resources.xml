<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
            <service id="shopware.shoptemplate.shop-template.resource" class="Shopware\ShopTemplate\Gateway\Resource\ShopTemplateResource">
        <tag name="shopware.framework.write.resource"/> 
    </service>
    <service id="shopware.shoptemplate.shop-template-config-form.resource" class="Shopware\ShopTemplate\Gateway\Resource\ShopTemplateConfigFormResource">
        <tag name="shopware.framework.write.resource"/> 
    </service>
    <service id="shopware.shoptemplate.shop-template-config-form-field.resource" class="Shopware\ShopTemplate\Gateway\Resource\ShopTemplateConfigFormFieldResource">
        <tag name="shopware.framework.write.resource"/> 
    </service>
    <service id="shopware.shoptemplate.shop-template-config-form-field-value.resource" class="Shopware\ShopTemplate\Gateway\Resource\ShopTemplateConfigFormFieldValueResource">
        <tag name="shopware.framework.write.resource"/> 
    </service>
    <service id="shopware.shoptemplate.shop-template-config-preset.resource" class="Shopware\ShopTemplate\Gateway\Resource\ShopTemplateConfigPresetResource">
        <tag name="shopware.framework.write.resource"/> 
    </service>
    </services>
</container>