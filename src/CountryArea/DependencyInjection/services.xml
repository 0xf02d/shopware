<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service class="Shopware\CountryArea\Struct\CountryAreaHydrator" id="shopware.country_area.struct.country_area_hydrator"/>
        <service class="Shopware\CountryArea\Gateway\CountryAreaReader" id="shopware.country_area.gateway.country_area_reader">
            <argument id="dbal_connection" type="service"/>
            <argument id="shopware.framework.struct.field_helper" type="service"/>
            <argument id="shopware.country_area.struct.country_area_hydrator" type="service"/>
        </service>
        <service class="Shopware\CountryArea\Gateway\CountryAreaRepository" id="shopware.country_area.gateway.country_area_repository">
            <argument id="shopware.country_area.gateway.country_area_reader" type="service"/>
        </service>

    </services>
</container>