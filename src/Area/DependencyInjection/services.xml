<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
<service class="Shopware\Area\Reader\AreaBasicHydrator" id="shopware.area.reader.basic_hydrator">

</service>
<service class="Shopware\Area\Reader\AreaBasicReader" id="shopware.area.reader.basic_reader">
  <argument id="dbal_connection" type="service"/>
  <argument id="shopware.area.reader.basic_hydrator" type="service"/>
</service>
<service class="Shopware\Area\Loader\AreaBasicLoader" id="shopware.area.loader.basic_loader">
    <argument type="service" id="shopware.area.reader.basic_reader" />

</service>
<service class="Shopware\Area\Searcher\AreaSearcher" id="shopware.area.searcher.searcher">
  <argument id="dbal_connection" type="service"/>
  <argument type="collection">
        <!-- #handlers# -->
  </argument>
  <argument id="shopware.area.reader.basic_hydrator" type="service"/>
</service>
<service class="Shopware\Area\Writer\AreaWriter" id="shopware.area.writer.writer"/>
<service class="Shopware\Area\AreaRepository" id="shopware.area.repository">
  <argument id="shopware.area.loader.basic_loader" type="service"/>
  <argument id="event_dispatcher" type="service"/>
  <argument id="shopware.area.searcher.searcher" type="service"/>
  <argument id="shopware.area.writer.writer" type="service"/>
  <argument id="shopware.area.loader.detail_loader" type="service"/>
</service>
<service class="Shopware\Area\Loader\AreaDetailLoader" id="shopware.area.loader.detail_loader">
    <argument type="service" id="shopware.area.loader.basic_loader" />
    <argument type="service" id="shopware.area_country.searcher.searcher" />
</service>

    </services>
</container>