<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service class="Shopware\Category\Command\RebuildTreeCommand" id="shopware.category.command.rebuild_category_tree_command">
            <tag name="console.command"/>
        </service>

        <service class="Shopware\Category\Denormalization\CategoryPathBuilder" id="shopware.category.denormalization.category_path_builder">
            <argument id="shopware.category.repository" type="service"/>
        </service>
    </services>
</container>