<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
<service class="Shopware\CustomerGroupDiscount\Reader\CustomerGroupDiscountBasicHydrator" id="shopware.customer_group_discount.reader.basic_hydrator">

</service>
<service class="Shopware\CustomerGroupDiscount\Reader\CustomerGroupDiscountBasicReader" id="shopware.customer_group_discount.reader.basic_reader">
  <argument id="dbal_connection" type="service"/>
  <argument id="shopware.customer_group_discount.reader.basic_hydrator" type="service"/>
</service>
<service class="Shopware\CustomerGroupDiscount\Loader\CustomerGroupDiscountBasicLoader" id="shopware.customer_group_discount.loader.basic_loader">
    <argument type="service" id="shopware.customer_group_discount.reader.basic_reader" />

</service>
<service class="Shopware\CustomerGroupDiscount\Searcher\CustomerGroupDiscountSearcher" id="shopware.customer_group_discount.searcher.searcher">
  <argument id="dbal_connection" type="service"/>
  <argument type="collection">
        <!-- #handlers# -->
  </argument>
  <argument id="shopware.customer_group_discount.reader.basic_hydrator" type="service"/>
</service>
<service class="Shopware\CustomerGroupDiscount\Writer\CustomerGroupDiscountWriter" id="shopware.customer_group_discount.writer.writer"/>
<service class="Shopware\CustomerGroupDiscount\CustomerGroupDiscountRepository" id="shopware.customer_group_discount.repository">
  <argument id="shopware.customer_group_discount.loader.basic_loader" type="service"/>
  <argument id="event_dispatcher" type="service"/>
  <argument id="shopware.customer_group_discount.searcher.searcher" type="service"/>
  <argument id="shopware.customer_group_discount.writer.writer" type="service"/>
  
</service>

    </services>
</container>