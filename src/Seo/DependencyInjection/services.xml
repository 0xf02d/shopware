<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service class="Shopware\Seo\Generator\SeoUrlGeneratorRegistry" id="shopware.seo.url_generator_registry">
            <argument type="collection">
                <argument id="shopware.storefront.listing_page.listing_page_url_generator" type="service" />
                <argument id="shopware.storefront.detail_page.detail_page_url_generator" type="service" />
            </argument>
            <argument id="Shopware\Api\Seo\Repository\SeoUrlRepository" type="service"/>
            <argument id="dbal_connection" type="service"/>
            <argument id="Shopware\Api\Shop\Repository\ShopRepository" type="service"/>
        </service>

        <service class="Shopware\Seo\Extension\UrlGeneratorExtension" id="shopware.seo.extension.url_generator_extension">
            <argument id="router.request_context" type="service"/>
            <tag name="kernel.event_subscriber"/>
            <tag name="shopware.entity.extension"/>
        </service>


    </services>
</container>
