<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service class="Shopware\ShippingMethod\Struct\ShippingMethodHydrator" id="shopware.shipping_method.struct.shipping_method_hydrator">
            <argument id="shopware.framework.struct.attribute_hydrator" type="service"/>
        </service>
        <service class="Shopware\ShippingMethod\Gateway\ShippingMethodReader" id="shopware.shipping_method.gateway.shipping_method_reader">
            <argument id="shopware.framework.struct.field_helper" type="service"/>
            <argument id="shopware.shipping_method.struct.shipping_method_hydrator" type="service"/>
            <argument id="dbal_connection" type="service"/>
        </service>
        <service class="Shopware\ShippingMethod\Gateway\ShippingMethodRepository" id="shopware.shipping_method.gateway.shipping_method_repository">
            <argument id="shopware.shipping_method.gateway.shipping_method_reader" type="service"/>
        </service>

    </services>
</container>