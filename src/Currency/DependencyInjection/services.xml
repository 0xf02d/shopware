<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
<service class="Shopware\Currency\Reader\CurrencyBasicHydrator" id="shopware.currency.reader.basic_hydrator">

</service>
<service class="Shopware\Currency\Reader\CurrencyBasicReader" id="shopware.currency.reader.basic_reader">
  <argument id="dbal_connection" type="service"/>
  <argument id="shopware.currency.reader.basic_hydrator" type="service"/>
</service>
<service class="Shopware\Currency\Loader\CurrencyBasicLoader" id="shopware.currency.loader.basic_loader">
    <argument type="service" id="shopware.currency.reader.basic_reader" />

</service>
<service class="Shopware\Currency\Searcher\CurrencySearcher" id="shopware.currency.searcher.searcher">
  <argument id="dbal_connection" type="service"/>
  <argument type="collection">
        <!-- #handlers# -->
  </argument>
  <argument id="shopware.currency.reader.basic_hydrator" type="service"/>
</service>
<service class="Shopware\Currency\Writer\CurrencyWriter" id="shopware.currency.writer.writer"/>
<service class="Shopware\Currency\CurrencyRepository" id="shopware.currency.repository">
  <argument id="shopware.currency.loader.basic_loader" type="service"/>
  <argument id="event_dispatcher" type="service"/>
  <argument id="shopware.currency.searcher.searcher" type="service"/>
  <argument id="shopware.currency.writer.writer" type="service"/>
  
</service>

    </services>
</container>