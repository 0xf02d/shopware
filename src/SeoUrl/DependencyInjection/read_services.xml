<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service class="Shopware\SeoUrl\Factory\SeoUrlBasicFactory" id="shopware.seo_url.basic_factory">
            <argument id="dbal_connection" type="service"/>
            <argument type="collection" />
        </service>

        <service class="Shopware\SeoUrl\Loader\SeoUrlBasicLoader" id="shopware.seo_url.basic_loader">
            <argument id="shopware.seo_url.basic_factory" type="service"/>
        </service>

        <service class="Shopware\SeoUrl\Searcher\SeoUrlSearcher" id="shopware.seo_url.searcher">
            <argument id="dbal_connection" type="service"/>
            <argument id="shopware.search.sql_parser" type="service"/>
            <argument id="shopware.seo_url.basic_factory" type="service"/>
            <argument id="shopware.seo_url.basic_loader" type="service"/>
        </service>

        <service class="Shopware\SeoUrl\Repository\SeoUrlRepository" id="shopware.seo_url.repository">
            <argument id="shopware.seo_url.basic_loader" type="service"/>
            <argument id="event_dispatcher" type="service"/>
            <argument id="shopware.seo_url.searcher" type="service"/>
        </service>

    </services>
</container>