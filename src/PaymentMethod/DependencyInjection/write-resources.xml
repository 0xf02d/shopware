           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
            <service id="shopware.paymentmethod.payment-method.resource" class="Shopware\PaymentMethod\Gateway\Resource\PaymentMethodResource">
        <tag name="shopware.framework.write.resource"/> 
    </service>
    <service id="shopware.paymentmethod.payment-method-country.resource" class="Shopware\PaymentMethod\Gateway\Resource\PaymentMethodCountryResource">
        <tag name="shopware.framework.write.resource"/> 
